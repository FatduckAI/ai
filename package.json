{
  "name": "@fatduckai/ai",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "scope": "@fatduckai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fatduckai/ai.git"
  },
  "scripts": {
    "build": "bun run build:clean && bun run build:js && bun run build:types",
    "build:clean": "rm -rf dist",
    "build:js": "bun build ./src/core/ai.ts --outdir ./dist --target node --sourcemap=external",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "watch": "bun build ./src/core/ai.ts --outdir ./dist --target node --watch",
    "dev": "bun --watch src/core/ai.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepack": "bun run build",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "bun run build && changeset publish",
    "release": "changeset publish",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "ducky:start": "bun run src/agent/index.ts"
  },
  "version": "0.7.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "prompt-engineering",
    "llm",
    "ai",
    "template"
  ],
  "author": "Fat Duck AI",
  "license": "MIT",
  "dependencies": {
    "@fatduckai/prompt-utils": "^0.4.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.3",
    "drizzle-zod": "^0.5.1",
    "figlet": "^1.8.0",
    "glob": "^11.0.0",
    "openai": "^4.72.0",
    "postgres": "^3.4.5",
    "readline": "^1.3.0",
    "telegraf": "^4.16.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@changesets/cli": "^2.27.9",
    "@types/bun": "^1.1.13",
    "@types/figlet": "^1.7.0",
    "bun-types": "latest",
    "drizzle-kit": "^0.28.1",
    "typescript": "^5.3.3"
  }
}
